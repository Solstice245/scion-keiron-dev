    <StateGroup name="SupplyPreciseUnitListTemplate">
        <DefaultState val="Default"/>
        <!--
            Unit List - Supply Precision Label In Command Tooltip
        -->
        <State name="RevenantBomb">
            <When type="Property" frame="$parent/CommandButtonImage" Text="Assets\Textures\btn-unit-genetron-jupiterbomb.dds"/>
            <Action type="SetProperty" Unit="JupiterRevenantBomb"/>
            <Action type="SetProperty" frame="$parent/SupplyLabel" Alpha="0"/>
        </State>
        <State name="Fuse">
            <When type="Property" frame="$parent/CommandButtonImage" Text="Assets\Textures\btn-unit-keiron-fuse.dds"/>
            <Action type="SetProperty" Unit="Fuse"/>
            <Action type="SetProperty" frame="$parent/SupplyLabel" Alpha="0"/>
        </State>
        <State name="Xayidling">
            <When type="Property" frame="$parent/CommandButtonImage" Text="Assets\Textures\btn-unit-xayid-xayidling.dds"/>
            <Action type="SetProperty" Unit="Xayidling"/>
